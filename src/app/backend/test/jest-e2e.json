{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "moduleNameMapper": {
    "src/(.*)": "<rootDir>/../src/$1"
  },
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": [
    "**/*.(t|j)s"
  ],
  "coverageDirectory": "../coverage-e2e",
  "testTimeout": 60000,
  "maxWorkers": 1,
  "forceExit": true,
  "detectOpenHandles": false
}